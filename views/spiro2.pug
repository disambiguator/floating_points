doctype html
html(lang='en')
  include includes/head.pug
  body
    label Speed
    input(type='range', min='0', max='5000', step='1', onchange='setRenderSpeed(this.value)')
    button(onclick='toggleColor(this)') Enable Color
    button(onclick='depthGlitch()') Depth Glitch
    input(type='range', min='-100', max='1000', onclick='zSlider(this)')
    div#spiro
    script(type='text/javascript', src='dist/main.js')
